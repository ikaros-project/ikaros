<!-- temp -->
	<module class = "Constant" name = "temp"	data = "0 0 0" />
	<module class = "Constant" name = "temp2"	data = "0" />
	<connection  source = "temp.OUTPUT"  target = "LED.EYE_COLOR"  size = "3"/>
	<connection  source = "temp.OUTPUT"  target = "LED.MOUTH_COLOR" size = "3"/>

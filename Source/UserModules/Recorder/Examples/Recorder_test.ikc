<?xml version="1.0"?>

<group name="Recorder Test">
	
    <module
        class="FunctionGenerator"
        name="F"
        type = "sin"
        frequency="0.53"
        basetime = "20"
        tickduty = "8"
        amplitude = "1"
    />


	<module
		class = "Recorder"
		name = "Recorder"
        length = "100"
        count = "10"
        min="-1"
        filename="test.csv"
	/>
	
	
	<connection sourcemodule="F" source="OUTPUT" targetmodule="Recorder" target="INPUT" />

	<view name="Recorder View">
        <object class="Path" order="col" title="Recorder" module="Recorder" source="OUTPUT" x="0" y="0" w="6" h="4" />
    </view>
</group>
 

<?xml version="1.0"?>

<group title="NucleusEnsemble Example">
	
		<module
			class = "Constant"
			name = "MyConstant"
			outputsize  = "2"
			data  = "1 2"
		/>
		<module
		  class       =   "FunctionGenerator"
		  name        =   "SquareGen"
		  type        =   "square"
		  frequency  =  "0.001"
		  offset      =   "0.0"
		  amplitude   =   "1"
		  shift       =   "0.0"
		/>

		<module
			class = "NucleusEnsemble"
			name = "NucleusEnsembleInstance"
			parameter1  = "1.23"
			parameter2  = "12"
		/>
		
        <connection sourcemodule = "MyConstant" source = "OUTPUT" targetmodule = "NucleusEnsembleInstance" target = "INPUT1" />
        <connection sourcemodule = "SquareGen" source = "OUTPUT" targetmodule = "NucleusEnsembleInstance" target = "INPUT2" />

		<view name="View">
			<object class="Plot " 
				module="SquareGen" 
				source="OUTPUT" 
				x="0" y="0" w="1" h="1"
				min="0"
				max="2"
			/>
			<object class="Plot " 
				module="NucleusEnsembleInstance" 
				source="OUTPUT" 
				x="0" y="1" w="1" h="1"
				min="0"
				max="2"
			/>
		</view>

</group>
 

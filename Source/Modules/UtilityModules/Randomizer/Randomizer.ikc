<class name="Randomizer" description="outputs a random value">
	<parameter name="size" type="matrix" default="11,22" description="output from module" />	<!-- FIXME: DEFAULT VALUES DOES NOT WORK FOR SIZES -->
	<parameter name="min" type="float" description="Minimum value of the output (inclusive)" default="0" />
	<parameter name="max" type="float" description="Maximum value of the output (inclusive)" default="1" />

	<output name="OUTPUT" description="The output" size="@size"/>
</class>

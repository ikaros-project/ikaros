<?xml version="1.0"?>

<group name="Test">
	
	<module class = "Randomizer" name = "Constant1" outputsize = "5" max="4" />
	<module class = "Randomizer" name = "Constant2" outputsize = "5" max="4" />

	<module
		class = "Add"
		name = "Add"
	/>
	
	<connection sourcemodule="Constant1" source="OUTPUT" targetmodule="Add" target="INPUT1" />
	<connection sourcemodule="Constant2" source="OUTPUT" targetmodule="Add" target="INPUT2" />

	<view name="View">

	<table
		class = "table"
		height = "300"
		title = "A"
		width = "300"
		source = "Constant1.OUTPUT"
		y = "20"
		x = "40"
		decimals = "4"
		colorize = "true"
		show_title = "true"
		show_frame = "true"
	/>

	<table
		class = "table"
		height = "300"
		title = "B"
		width = "300"
		source = "Constant2.OUTPUT"
		y = "20"
		x = "360"
		decimals = "4"
		colorize = "true"
		show_title = "true"
		show_frame = "true"
	/>

	<table
		class = "table"
		height = "300"
		title = "Sum"
		width = "300"
		source = "Add.OUTPUT"
		y = "360"
		x = "200"
		decimals = "4"
		colorize = "true"
		show_title = "true"
		show_frame = "true"
	/>

	</view>
    <module class = "Logger" name = "Logger" />
</group>
 

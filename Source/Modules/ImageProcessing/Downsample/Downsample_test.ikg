<?xml version="1.0"?>

<group name="Downsample_Example">


	<module
		class="InputVideo" 
		name="InputVideo" 
		_frame_rate="30" 
		list_devices = "yes"
		size_x = "640"
		size_y = "480"
		_size_x = "1280"
		_size_y = "720"
	/>





	<module
			class = "Downsample"
			name = "Downsample"
	/>


	<connection  source = "InputVideo.OUTPUT"  target = "Downsample.INPUT" _delay="1" />


</group>
 

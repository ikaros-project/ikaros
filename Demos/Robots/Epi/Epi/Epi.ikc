                scales = "no"
            />

            <grid
                width = "121"
                height = "41"
                x="10"
                y = "40"
                source = "PLAYING"
                module = "MotionRecorder"
                class = "grid"
                max = "1"
                labels = "play"
                labelWidth = "50"
                fill = "custom"
                colorTable = "white, red, green"
                lineWidth = "1"
                shape = "circle"
                size = "1"
                scales = "no"
            />

            <plot module="MotionRecorder" source="OUTPUT" min="-360" max="360" color="red" select="0, 1" x="130" y="0" width="800" height="501" />


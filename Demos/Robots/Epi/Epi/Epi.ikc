<connection sourcemodule = "Actions"   	source = "TRIGGER"  targetmodule = "MotionRecorder"  target = "TRIG" />
<connection sourcemodule = "MotionRecorder"   	source = "COMPLETED"  targetmodule = "Actions"  target = "COMPLETE" />

